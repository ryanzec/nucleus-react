{
  "name": "nucleus-react",
  "version": "0.11.0-rc1",
  "main": "index",
  "keywords": [
    "react",
    "reactjs",
    "components"
  ],
  "author": "Ryan Zec <code@ryanzec.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryanzec/nucleus-react.git"
  },
  "dependencies": {
    "axios": "0.17.0",
    "bluebird": "3.4.1",
    "font-awesome-svg-icons": "0.1.0",
    "form-data-validation": "0.10.2",
    "history": "3.3.0",
    "holderjs": "2.9.4",
    "immutable": "3.8.1",
    "jsuri": "1.3.1",
    "lodash": "4.17.4",
    "messageformat": "0.3.1",
    "moment": "2.14.1",
    "moment-timezone": "0.5.5",
    "popper.js": "1.10.8",
    "prop-types": "15.6.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-transition-group": "1.2.0",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.5",
    "react-router-scroll": "0.4.3",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "store-cacheable": "0.1.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "ava": "0.22.0",
    "babel-cli": "6.24.1",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-static-fs": "1.1.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "babel-runtime": "6.25.0",
    "browser-env": "3.2.1",
    "chai": "4.1.2",
    "css-loader": "0.28.4",
    "css-modules-require-hook": "4.2.2",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-15": "1.0.2",
    "escope": "3.6.0",
    "eslint": "4.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "0.11.2",
    "glob-array": "1.0.0",
    "html-webpack-plugin": "2.29.0",
    "json-server": "0.12.0",
    "mocha": "3.0.2",
    "mock-webstorage": "1.0.3",
    "node-sass": "4.5.3",
    "node-sass-package-importer": "3.0.4",
    "postcss-loader": "2.0.6",
    "raf": "3.4.0",
    "react-dnd": "2.4.0",
    "react-dnd-html5-backend": "2.4.1",
    "redux-freeze": "0.1.5",
    "resolve-url-loader": "2.1.0",
    "sass-loader": "6.0.6",
    "sinon": "1.17.5",
    "string.format": "1.3.1",
    "style-loader": "0.18.2",
    "wdio-mocha-framework": "0.4.0",
    "wdio-spec-reporter": "0.0.3",
    "webdriver-manager": "12.0.6",
    "webdriverio": "4.2.7",
    "webpack": "3.5.4",
    "webpack-clean-obsolete-chunks": "0.1.10",
    "webpack-livereload-plugin": "0.11.0"
  },
  "babel": {
    "presets": [
      "react",
      "babel-preset-es2015-without-strict-loose"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "static-fs"
    ]
  },
  "engines": {
    "node": ">=4.4.3"
  },
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/extensionOverrides.js"
    ],
    "babel": "inherit",
    "failFast": true
  },
  "scripts": {
    "build-developer": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run i18n && npm run webpack",
    "build-production": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run i18n && npm run webpack-production",
    "build-watch": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run i18n && npm run webpack-watch",
    "clean": "echo \"Cleaning out build directory...\" && rm -rf ./web/build",
    "copy-static-assets": "echo \"Copying static assets...\" && mkdir -p ./web/build/images && cp -R ./web/app/images/ ./web/build/images && cp -R ./src/fonts/ ./web/build/fonts",
    "eslint": "echo \"Linting javascript code...\" && ./node_modules/.bin/eslint --ext .js --ext .jsx src/ web/app",
    "eslint-fix": "echo \"Build i18n files...\" && ./node_modules/.bin/eslint --fix --ext .js --ext .jsx src/ web/app",
    "i18n": "rm -rf web/build/locale && mkdir -p web/build/locale && ./node_modules/.bin/messageformat --locale en --module web/locale/en --output test/i18n-node.js && ./node_modules/.bin/messageformat --locale en web/locale/en --output web/build/locale/en-i18n.js",
    "prepublish": "rm -rf ./lib && mkdir -p ./lib/styles && ./node_modules/babel-cli/bin/babel.js src --out-dir lib && cp -r ./src/styles/ ./lib/styles && cp -r ./src/fonts/ ./lib/fonts",
    "ui-tests": "./node_modules/.bin/wdio ./ui-tests/configurations/wdio.config.js",
    "unit-tests": "NODE_PATH=. ./node_modules/.bin/ava test/**/*.spec.js --verbose",
    "unit-tests-update": "NODE_PATH=. ./node_modules/.bin/ava test/**/*.spec.js --verbose --update-sna",
    "webpack": "./node_modules/.bin/webpack --progress --config webpack.config.js",
    "webpack-production": "WEBPACK_PRODUCTION_BUILD=1 ./node_modules/.bin/webpack --progress --config webpack.config.js",
    "webpack-watch": "./node_modules/.bin/webpack --progress --config webpack.config.js --watch"
  }
}
